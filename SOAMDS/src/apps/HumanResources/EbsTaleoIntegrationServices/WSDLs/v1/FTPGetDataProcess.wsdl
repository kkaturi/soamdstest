<?xml version= '1.0' encoding= 'UTF-8' ?>
<wsdl:definitions
     name="GetdataProcess"
     targetNamespace="http://xmlns.oracle.com/FileTransferCommonServices/GetData/GetdataProcess"
     xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
     xmlns:client="http://xmlns.oracle.com/FileTransferCommonServices/GetData/GetdataProcess"
     xmlns:ns1="http://xmlns.oracle.com/FileTransferCommonServices/GetData/GetDataProcess"
     xmlns:plnk="http://docs.oasis-open.org/wsbpel/2.0/plnktype"
     xmlns:ns2="http://TargetNamespace.com/CallFTPDetailsAdapter"
    >
    <plnk:partnerLinkType name="GetdataProcess">
        <plnk:role name="GetdataProcessProvider" portType="client:GetdataProcess"/>
        <plnk:role name="GetdataProcessRequester" portType="client:GetdataProcessCallback"/>
    </plnk:partnerLinkType>
    <wsdl:types>
        <schema xmlns="http://www.w3.org/2001/XMLSchema">
            <import namespace="http://xmlns.oracle.com/FileTransferCommonServices/GetData/GetDataProcess"
                 schemaLocation="../../Schemas/v1/FileDetails.xsd"/>
        </schema>
        <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:import namespace="http://TargetNamespace.com/CallFTPDetailsAdapter" schemaLocation="../../Schemas/v1/SourceFileSchema.xsd"/>
        </xsd:schema>
    </wsdl:types>
    <wsdl:message name="GetdataProcessRequestMessage">
        <wsdl:part name="payload" element="ns1:process"/>
    </wsdl:message>
    <wsdl:message name="GetdataProcessResponseMessage">
        <wsdl:part name="payload" element="ns1:processResponse"/>
    </wsdl:message>
    <wsdl:portType name="GetdataProcess">
        <wsdl:operation name="process">
            <wsdl:input message="client:GetdataProcessRequestMessage"/>
        </wsdl:operation>
    </wsdl:portType>
    <wsdl:portType name="GetdataProcessCallback">
        <wsdl:operation name="processResponse">
            <wsdl:input message="client:GetdataProcessResponseMessage"/>
        </wsdl:operation>
    </wsdl:portType>
</wsdl:definitions>
