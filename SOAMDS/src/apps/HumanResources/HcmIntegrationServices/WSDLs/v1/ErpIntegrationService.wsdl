<wsdl:definitions name="ErpIntegrationService" targetNamespace="http://xmlns.oracle.com/apps/financials/commonModules/shared/model/erpIntegrationService/" xmlns:plnk="http://schemas.xmlsoap.org/ws/2003/05/partner-link/" xmlns:errors="http://xmlns.oracle.com/adf/svc/errors/" xmlns:orafault="http://xmlns.oracle.com/oracleas/schema/oracle-fault-11_0" xmlns:tns="http://xmlns.oracle.com/apps/financials/commonModules/shared/model/erpIntegrationService/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:types="http://xmlns.oracle.com/apps/financials/commonModules/shared/model/erpIntegrationService/types/">
<wsdl:documentation xmlns:oer="http://xmlns.oracle.com/oer">
<name>ERP Integration Service</name>
<description>provides external web service operations for ERP integration
            scenarios.</description>
<docCategories>
<category>External</category>
</docCategories>
<oer:lifecycle>Active</oer:lifecycle>
<oer:compatibility>Supported - Backward Compatibility Assured</oer:compatibility>
<oer:operation name="getESSJobStatus">
<description>Obtains the request status of the submitted ESS job.</description>
<oer:lifecycle>Active</oer:lifecycle>
<oer:parameters>
<oer:parameter name="requestId" description="The request ID of the submitted ESS job."/>
<oer:return description="Returns the current status of the ESS job."/>
</oer:parameters>
</oer:operation>
<oer:operation name="getDfltObjAttrHints">
<description>An operation to retrieve user interface hints, such as the
                service data object label and object attribute labels, for the
                specified service data object and locale.  As a prerequisite,
                invoke the getEntityList operation defined on this service to
                get the list of possible values for the viewName request payload
                element.</description>
<oer:lifecycle>Active</oer:lifecycle>
<oer:parameters>
<oer:parameter name="viewName" description="The service view usage name, an internal name for the service data object.  Specify the value of the &lt;name> element from the getEntityList operation response payload."/>
<oer:parameter name="localeName" description="The locale to use when evaluating locale-based user interface hints.  The localeName is in ISO 639-1 format."/>
<oer:return description="The label and user interface hints for the specified service data object and its attributes in a name-value pair format"/>
</oer:parameters>
</oer:operation>
<oer:operation name="getServiceLastUpdateTime">
<description>An operation that returns the date and time when the schema
                files referenced in the service definition last changed.</description>
<oer:lifecycle>Active</oer:lifecycle>
<oer:parameters>
<oer:return description="The date and time when the service definition last changed in ISO 8601 format."/>
</oer:parameters>
</oer:operation>
<oer:operation name="getEntityList">
<description>An operation to get the list of service data objects defined on
                this service.</description>
<oer:lifecycle>Active</oer:lifecycle>
<oer:parameters>
<oer:return description="A list of service data objects defined on this service.  For each service data object, the result includes the QName, internal name, and Boolean values indicating whether create, update, merge, and delete operations can be performed on the object."/>
</oer:parameters>
</oer:operation>
<oer:operation name="submitESSJobRequest">
<description>Submits an ESS job request for the specified job definition.</description>
<oer:lifecycle>Active</oer:lifecycle>
<oer:parameters>
<oer:parameter name="jobPackageName" description="The ESS job package name for the ESS job that must be submitted."/>
<oer:parameter name="jobDefinitionName" description="The ESS job definition name for the ESS job that must be submitted."/>
<oer:parameter name="paramList" description="List of parameters used to invoke the ESS job. The order of the parameters is maintained as per the list.   The corresponding entry in the list should be blank when a given parameter is not passed."/>
<oer:return description="Returns the request ID of the submitted ESS job."/>
</oer:parameters>
</oer:operation>
<oer:operation name="submitJobWithOutput">
<description>Submits an ESS job to export and uploads the job output to the
                Universal Content Management server.</description>
<oer:lifecycle>Active</oer:lifecycle>
<oer:parameters>
<oer:parameter name="jobPackageName" description="The ESS job package name for the ESS job that must be submitted."/>
<oer:parameter name="jobDefinitionName" description="The ESS job definition name for the ESS job that must be submitted."/>
<oer:parameter name="callbackURL" description="The callback URL of the service implemented by the customers to receive the ESS job status on job completion."/>
<oer:parameter name="paramList" description="List of parameters used to invoke the ESS job. The order of the parameters is maintained as per the list.  The corresponding entry in the list should be blank when a given parameter is not passed."/>
<oer:return description="Returns the request ID of the submitted ESS job."/>
</oer:parameters>
</oer:operation>
<oer:operation name="getDocumentIdsForFilePrefix">
<description>Gets the document identifiers for files that match the file
                prefix and specified account. These files do not contain the
                specified comments.</description>
<oer:lifecycle>Active</oer:lifecycle>
<oer:parameters>
<oer:parameter name="prefix" description="The file prefix used to search for the files that start with a specificed prefix."/>
<oer:parameter name="account" description="The account parameter used to search for the files that belongs to a specified security account."/>
<oer:parameter name="comments" description="The parameter is used to pass the comments. Files that do not have the comments are retreived."/>
<oer:return description="Returns a list of document Identifiers."/>
</oer:parameters>
</oer:operation>
<oer:operation name="getDocumentForDocumentId">
<description>The operation is used to get the document that matches the given
                Document Identifier</description>
<oer:lifecycle>Active</oer:lifecycle>
<oer:parameters>
<oer:parameter name="DocumentId" description="The numeric document identifier of a file."/>
<oer:return description="Returns a list of elements, each containing the details of the downloaded files.  The details include the document ID, file content, file name, content type, file title, author, security group, and account."/>
</oer:parameters>
</oer:operation>
<oer:operation name="getDocumentsForFilePrefix">
<description>Gets the files that match the file prefix and specified account.
                These files do not contain the specified comments.</description>
<oer:lifecycle>Active</oer:lifecycle>
<oer:parameters>
<oer:parameter name="prefix" description="The file prefix used to search for the files that start with a specificed prefix."/>
<oer:parameter name="account" description="The account parameter used to search for the files that belongs to a specified security account."/>
<oer:parameter name="comments" description="The comments that are not present in the files."/>
<oer:return description="Returns a list of elements, each containing the details of the downloaded files.  The details include the document ID, file content, file name, content type, file title, author, security group, and account."/>
</oer:parameters>
</oer:operation>
<oer:operation name="uploadFileToUcm">
<description>Uploads a file to the Universal Content Management server based
                on the document specified.</description>
<oer:lifecycle>Active</oer:lifecycle>
<oer:parameters>
<oer:parameter name="document" description="List of elements, each containing the details of the file to be uploaded.  The details include the file content, file name, content type, file title, author, security group, and account."/>
<oer:return description="Returns the document ID of the uploaded file."/>
</oer:parameters>
</oer:operation>
<oer:operation name="downloadESSJobExecutionDetails">
<description>Downloads the ESS job output and the logs as a zip file.</description>
<oer:lifecycle>Active</oer:lifecycle>
<oer:parameters>
<oer:parameter name="requestId" description="The request ID of the submitted ESS job."/>
<oer:parameter name="fileType" description="The file type used to determine the execution details to download.  Specify the file type as log to download the log file and output file. Specify the file type as out to download the output file.  Download the log files and output files when no file type is specified."/>
<oer:return description="Returns a list of elements, each containing the details of the downloaded files. The details include the document ID, file content, file name, content type, file title, author, security group, and account."/>
</oer:parameters>
</oer:operation>
<oer:operation name="appendFileComment">
<description>Appends the specified comments to a file.</description>
<oer:lifecycle>Active</oer:lifecycle>
<oer:parameters>
<oer:parameter name="documentId" description="The numeric document identifier of a file."/>
<oer:parameter name="comments" description="Appends the specified comments to a file."/>
</oer:parameters>
</oer:operation>
<oer:operation name="downloadExportOutput">
<description>The method downloads the output of ESS job</description>
<oer:lifecycle>Deprecated</oer:lifecycle>
<oer:parameters>
<oer:parameter name="requestId" description="The parameter is used to pass the request id of the ESS job."/>
<oer:return description="The list of document details for the document is returned"/>
</oer:parameters>
</oer:operation>
<oer:operation name="loadAndImportData">
<description>Uploads a file to the Oracle Universal Content Management server
                based on the document specified and submits the ESS jobs to
                import and process the uploaded file.</description>
<oer:lifecycle>Active</oer:lifecycle>
<oer:parameters>
<oer:parameter name="document" description="List of elements, each containing the details of the file to be uploaded. The details include the file content, file name, content type, file title, author, security group, and account."/>
<oer:parameter name="jobList" description="List of elements, each containing the details of the ESS job to be submitted to import and process the uploaded file. The details include the job definition name, job package name, and list of parameters."/>
<oer:parameter name="interfaceDetails" description="The interface whose data is to be loaded."/>
<oer:parameter name="notificationCode" description="Two digit code representing the type and when a notification is to be sent"/>
<oer:parameter name="callbackURL" description="The callback URL of the service implemented by the customers to receive the ESS job status on job completion."/>
<oer:return description="Returns response code containing error details"/>
</oer:parameters>
</oer:operation>
<oer:operation name="confirmExtractConsumption">
<description>Retrieves the comment for a file with a given document Id from
                the UCM , parses the comment into fixed dimensions which is used
                to update the incremental date in the incremental  dimensions
                table based on the fixed dimensions. This operation is mainly
                used for Outbound Ess related jobs.</description>
<oer:lifecycle>Active</oer:lifecycle>
<oer:parameters>
<oer:parameter name="jobName" description="contains the package name and job name of the job separated by a comma"/>
<oer:parameter name="documentIDs" description="List of elements, each containing the details of the file to be uploaded. The details include the file content, file name, content type, file title, author, security group, and account."/>
<oer:return description="returns the status code of the operation"/>
</oer:parameters>
</oer:operation>
<oer:operation name="importBulkData">
<description>Uploads a file to the Oracle Universal Content Management server based on the document specified and submits the ESS jobs to import and process the uploaded file with multiple options.</description>
<oer:lifecycle>Active</oer:lifecycle>
<oer:parameters>
<oer:parameter name="document" description="List of elements, each containing the details of the file to be uploaded. The details include the file content, file name, content type, file title, author, security group, and account."/>
<oer:parameter name="jobDetails" description="List of elements, each containing the details of the ESS job to be submitted to import and process the uploaded file. The details include the job definition name, job package name, and list of parameters."/>
<oer:parameter name="notificationCode" description="Two digit code representing the type and when a notification is to be sent."/>
<oer:parameter name="callbackURL" description="The callback URL of the service implemented by the customers to receive the ESS job status on job completion."/>
<oer:parameter name="jobOptions" description="A web service operation input parameter. It designates the specific inbound bulk data processing variation selected and the output file type that will be generated upon completion of the data processing."/>
<oer:return description="Returns response code containing error details"/>
</oer:parameters>
</oer:operation>
<oer:operation name="extractAndPurge">
<description>Submits ESS jobs to upload the load and import job output to the Universal Content Management server and purge interface tables with callback and notification.</description>
<oer:lifecycle>Active</oer:lifecycle>
<oer:parameters>
<oer:parameter name="requestIds" description="The request Ids of load job and import jobs."/>
<oer:parameter name="notificationCode" description="Two digit code representing the type and when a notification is to be sent."/>
<oer:parameter name="callbackURL" description="The callback URL of the service implemented by the customers to receive the ESS job status on job completion."/>
<oer:parameter name="jobOptions" description="A web service operation input parameter which will allow new attributes to be used for future enhancements to ensure data model flexibility."/>
<oer:return description="Returns the request ID of the first submitted ESS job."/>
</oer:parameters>
</oer:operation>
<oer:operation name="exportBulkData">
<description>Submits an ESS job to export and uploads the job output to the Universal Content Management server with callback and notification.</description>
<oer:lifecycle>Active</oer:lifecycle>
<oer:parameters>
<oer:parameter name="jobName" description="The ESS job package name and definition name for the ESS job that must be submitted."/>
<oer:parameter name="parameterList" description="Parameters used to invoke the ESS job."/>
<oer:parameter name="jobOptions" description="A web service operation input parameter which will allow new attributes to be used for future enhancements to ensure data model flexibility."/>
<oer:parameter name="callbackURL" description="The callback URL of the service implemented by the customers to receive the ESS job status on job completion."/>
<oer:parameter name="notificationCode" description="Two digit code representing the type and when a notification is to be sent."/>
<oer:return description="Returns the request ID of the submitted ESS job."/>
</oer:parameters>
</oer:operation>
</wsdl:documentation>
