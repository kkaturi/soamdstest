<?xml version= '1.0' encoding= 'UTF-8' ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://xmlns.oracle.com/EventDetails"
     xmlns="http://xmlns.oracle.com/EventDetails" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <xs:element name="customEvent">
        <xs:annotation id="CustomEvent">
            <xs:documentation>Is available for the future Events.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="customMessage" type="xs:string">
                    <xs:annotation id="CustomMessage">
                        <xs:documentation>Is available for the future Events and can be used to extend the current
       EventDetails.xsd to cater future Event requirements.</xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>
